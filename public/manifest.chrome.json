{
	"manifest_version": 3,
	"name": "闲鱼助手",
	"version": "1.0.1",
	"description": "PC端打开闲鱼或者转转的h5页面时，自动打开一个手机尺寸比例的窗口浏览",
	"homepage_url": "https://github.com/TheKonka/goofish-browser-extension",
	"permissions": ["tabs"],
	"icons": {
		"16": "icons/icon-16.png",
		"32": "icons/icon-32.png",
		"48": "icons/icon-48.png",
		"96": "icons/icon-96.png",
		"128": "icons/icon-128.png"
	},
	"background": {
		"service_worker": "background.js"
	}
}
